{
    "name": "renatomarinho/laravel-gitscrum",
    "description": "GitScrum is a project to help developer team. Git + Scrum = Team More Productive ",
    "keywords": [
        "git",
        "scrum",
        "team",
        "developer",
        "tasks",
        "backlog",
        "issue",
        "laravel",
        "gitscrum"
    ],
    "license": "GPL-3.0",
    "minimum-stability": "dev",
    "type": "application",
    "authors": [
        {
            "name": "Renato Marinho",
            "email": "renato.marinho@s2move.com",
            "homepage": "http://renatomarinho.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "graham-campbell/github": "^4.4",
        "laravel/socialite": "^2.0",
        "nesbot/carbon": "^1.21",
        "phploc/phploc": "*",
        "barryvdh/laravel-debugbar": "^2.2",
        "intervention/image": "^2.3",
        "talevskiigor/composer-bump": "^1.0",
        "socialiteproviders/gitlab": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "way/generators": "dev-feature/laravel-five-stable",
        "xethron/migrations-generator": "^2.0",
        "squizlabs/php_codesniffer": "^2.7",
        "behat/behat": "^3.2"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models"
        ],
        "psr-4": {
            "GitScrum\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/support/DatabaseMigrations.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "https://github.com/jamisonvalenta/Laravel-4-Generators.git"
        }
    },
    "version": "0.10.0"
}
