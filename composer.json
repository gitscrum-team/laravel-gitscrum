{
    "name": "renatomarinho/laravel-gitscrum",
    "description": "GitScrum is a project to help developer team. Git + Scrum = Team More Productive ",
    "keywords": [
        "git",
        "scrum",
        "team",
        "developer",
        "tasks",
        "backlog",
        "issue",
        "laravel",
        "gitscrum"
    ],
    "license": "GPL-3.0",
    "minimum-stability": "dev",
    "type": "application",
    "authors": [
        {
            "name": "Renato Marinho",
            "email": "renato.marinho@s2move.com",
            "homepage": "http://renatomarinho.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/socialite": "^2.0",
        "phploc/phploc": "*",
        "intervention/image": "^2.3",
        "laravel/framework": "5.3.*",
        "socialiteproviders/gitlab": "^2.0",
        "nesbot/carbon": "^1.21",
        "doctrine/dbal": "~2.5",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "require-dev": {
        "talevskiigor/composer-bump": "^1.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "^2.7",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/dom-crawler": "3.2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "GitScrum\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/support/DatabaseMigrations.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "version": "0.13.0"
}
